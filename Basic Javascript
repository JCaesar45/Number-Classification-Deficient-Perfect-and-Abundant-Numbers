function getDPA(num) {
  let deficientCount = 0;
  let perfectCount = 0;
  let abundantCount = 0;

  // Helper function to calculate proper divisors sum
  function properDivisorsSum(n) {
    let sum = 1; // 1 is always a proper divisor (except for n=1)
    const sqrtN = Math.sqrt(n);
    for (let i = 2; i <= sqrtN; i++) {
      if (n % i === 0) {
        sum += i;
        const otherDivisor = n / i;
        if (otherDivisor !== i) {
          sum += otherDivisor;
        }
      }
    }
    // For n=1, proper divisors sum is 0
    if (n === 1) return 0;
    return sum;
  }

  for (let n = 1; n <= num; n++) {
    const sumDivisors = properDivisorsSum(n);
    if (sumDivisors < n) {
      deficientCount++;
    } else if (sumDivisors === n) {
      perfectCount++;
    } else {
      abundantCount++;
    }
  }

  return [deficientCount, perfectCount, abundantCount];
}
